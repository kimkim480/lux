constellation main

fn Prism() {
  let x: Light = 10;
  emit x; // test: <10>

  x = 42;
  emit x; // test: <42>

  x = x + 8;
  emit x; // test: <50>

  x = (x * 2) - 10;
  emit x; // test: <90>

  let y: Light = 1;
  y = y + x;
  emit y; // test: <91>

  let z: Light = 10;
  z **= 2;
  emit z;               // test: 100

  emit "123"::len();    // test: 3

  let s: Lumens = "Lux";
  let sub: Lumens = s[1..2];
  emit sub;             // test: "u"
}


