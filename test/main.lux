constellation main;

fn Prism() {
  let xs: [Light] = [1, 2];

  fn map(arr: [T], callback: Lambda) {
    for(let i: Light = 0; i < 2; i += 1) {
      
      let temp: Light = callback(arr[i]);
      arr[i] = temp;
    }
  }

  emit xs; 
  map(xs, fn(x: Light) { return x * 2; }); 
  emit xs;
}