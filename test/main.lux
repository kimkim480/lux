fn pow(base: Light, exp: Light) -> Light {
  let r: Light = 1;
  let curr_b: Light = base;
  let curr_e: Light = exp;

  for (;;) {
    if curr_e < 1 {
      break;
    }

    if curr_e % 2 == 1 {
      r = r * curr_b;
    }

    curr_e = curr_e / 2;
    curr_b = curr_b * curr_b;
  }

  return r;
}

